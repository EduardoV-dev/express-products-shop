<div class="grid">
  {{#each products}}
    <div class="card">
      <h3>{{title}}</h3>
      <img src="{{imageURL}}" alt="{{title}}" />
      <span>$ {{price}}</span>
      <p>{{description}}</p>
      <div class="card-actions">
        <a class="btn btn-primary" href="/shop/products/detail/12345">Details</a>
        <a class="btn btn-primary" href="/shop/products/add-to-cart">Add to Cart</a>
      </div>
    </div>
  {{/each}}